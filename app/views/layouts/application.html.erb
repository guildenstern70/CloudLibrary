<!doctype html>
<html lang="en">
<head>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  <title>The Cloud Library</title>
  <!-- Favicon -->
  <%= favicon_link_tag asset_path('library.png') %>
  <!-- JavaScripts loaded from Packs -->
  <%= yield :javascripts %>
  <!-- CSS loaded from Packs -->
  <%= yield :css %>
</head>
<body>
<main>
  <%= yield :content %>
</main>
</body>
</html>

